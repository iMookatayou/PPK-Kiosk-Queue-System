generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model DataQueue {
  date           DateTime @id         // ใช้ date เป็น PRIMARY KEY
  lastQueue      Int
  previousQueue  Int?    @default(0)
  source         String   @default("manual") // manual | auto
  location       String?
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
}
